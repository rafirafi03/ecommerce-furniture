            <div class="page-content" >
                <div class="cart">
                    <div class="container" >
                        <% if(product && product.product && product.product.length> 0) { %>
                            <div class="row" >
                                <div class="col-lg-9" >
                                    <table class="table table-cart table-mobile" >
                                        <thead>
                                            <tr>
                                                <th>Product</th>
                                                <th>Price</th>
                                                <th>Stocks</th>
                                                <th>Quantity</th>
                                                <th>Total</th>
                                                <th></th>
                                            </tr>
                                        </thead>

                                        <tbody >
                                            <% product.product.forEach((prod,index)=> { %>
                                                <tr >
                                                    <td class="product-col">
                                                        <div class="product">
                                                            <figure class="product-media">
                                                                <a href="#">
                                                                    <img src="/multerimages/<%= prod.productId.images[0] %>"
                                                                        alt="Product image">
                                                                </a>
                                                            </figure>

                                                            <h3 class="product-title">
                                                                <a>
                                                                    <%=prod.productId.name%>
                                                                </a>
                                                            </h3><!-- End .product-title -->
                                                        </div><!-- End .product -->
                                                    </td>
                                                    <%if(prod.productId.offerId){%>
                                                        
                                                        <td class="price-col text-success">
                                                            <p><s class="text-danger">₹ <%=prod.price%>/-</s></p>
                                                            ₹ <%=prod.productId.offerPercentage %>/-
                                                         </td>
                                                        <%}else{%>
                                                            <td class="price-col">
                                                                ₹ <%=prod.price %>.00
                                                             </td>
                                                        <%}%>
                                                    
                                                    <%if(prod.productId.quantity > 5) {%>
                                                    <td class="stock-col">
                                                        <span class="in-stock">In stock</span>
                                                      </td>
                                                    <%} else if(prod.productId.quantity < 6){%>
                                                        <td class="stock-col">
                                                            <span class="text-warning">Few left</span>
                                                          </td>
                                                    <%} else {%>
                                                        <td class="stock-col">
                                                            <span class="text-danger">Out of Stock</span>
                                                          </td>
                                                    <%}%>
                                                    <td class="quantity-col">
                                                        <div class="cart-product-quantity">
                                                            <input id="in<%=index%>"
                                                                onchange="quantityFnctn('<%=prod._id %>','<%=index%>','<%= prod.productId._id %>')"
                                                                type="number" class="form-control" value="<%=prod.quantity%>" min="1"
                                                                step="1"
                                                                data-decimals="0" required>

                                                        </div><!-- End .cart-product-quantity -->

                                                    </td>
                                                    <td class="total-col">
                                                       ₹ <%=prod.totalPrice %>
                                                    </td>
                                                    <td class="remove-col">
                                                        <button class="btn-remove"
                                                            data-product-id="<%= prod.productId._id %>"><i
                                                                class="icon-close"></i></button>
                                                    </td>

                                                </tr>
                                                <% }) %>
                                        </tbody>
                                    </table><!-- End .table table-wishlist -->

                                    <!-- <div class="cart-bottom">
                                        <div class="cart-discount">
                                            <form action="#">
                                                <div class="input-group">
