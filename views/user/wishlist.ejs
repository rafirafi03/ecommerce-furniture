        <div class="page-content">
          <div class="container">
            <% if(product && product.product && product.product.length> 0 ) {%>
            <table class="table table-wishlist table-mobile">
              <thead>
                <tr>
                  <th>Product</th>
                  <th>Price</th>
                  <th>Stock Status</th>
                  <th></th>
                  <th></th>
                </tr>
              </thead>

              <tbody>
                <% product.product.forEach((prod,index)=> { %>
                <tr>
                  <td class="product-col">
                    <div class="product">
                      <figure class="product-media">
                        <a href="#">
                          <img
                            src="/multerimages/<%= prod.productId.images[0] %>"
                            alt="Product image"
                          />
                        </a>
                      </figure>

                      <h3 class="product-title">
                        <a href="#"> <%= prod.productId.name %> </a>
                      </h3>
                      <!-- End .product-title -->
                    </div>
                    <!-- End .product -->
                  </td>
                  <%if(prod.productId.offerId){%>
                  <p class="text-danger"></p>
                  <td class="price-col text-success">
                    <s class="small text-danger"
                      >₹ <%= prod.productId.price %> /-</s
                    >
                    <br />₹ <%= prod.productId.offerPercentage %>/-
                  </td>
                  <%}else{%>
                  <td class="price-col"><%= prod.productId.price %></td>
                  <%}%> <%if(prod.productId.quantity> 5) {%>
                    <%if(prod.productId.isListed === false) {%>
                      <td class="stock-col">
                        <span class="text-danger">Not available</span>
                      </td>
                      <%}else{%>
                  <td class="stock-col">
                    <span class="text-success">In Stock</span>
                  </td>
                  <%}%>
                  <%} else if(prod.productId.quantity < 6){%>
                    <%if(prod.productId.isListed === false) {%>
                      <td class="stock-col">
                        <span class="text-danger">Not available</span>
                      </td>
                      <%}else{%>
                  <td class="stock-col">
                    <span class="text-warning">Few left</span>
                  </td>
                  <%}%>
                  <%} else {%>
                    <%if(prod.productId.isListed === false) {%>
                      <td class="stock-col">
                        <span class="text-danger">Not available</span>
                      </td>
                      <%}else{%>
                  <td class="stock-col">
                    <span class="text-danger">Out of Stock</span>
                  </td>
                  <%}%>
                  <%}%>
                  <%if(prod.productId.isListed === false) {%>
                    <td class="action-col">
                      <button disabled
                        type="button"
                        class="btn btn-block btn-outline-primary-2"
                      >
                        Not available
                      </button>
                    </td>
                    <%}else{%> 
                      <td class="action-col">
                        <button
                          type="button"
                          onclick="addToCart('<%= prod.productId._id %>')"
                          class="btn btn-block btn-outline-primary-2"
                        >
                          <i class="icon-cart-plus"></i>Add to Cart
                        </button>
                      </td>
                      <%}%>
                  <td class="remove-col">
                    <button
                      class="btn-remove"
                      data-product-id="<%= prod.productId._id %>"
                    >
                      <i class="icon-close"></i>
                    </button>
                  </td>
                </tr>
                <% }) %>
              </tbody>
            </table>

            <% } else { %>
            <div>
              <div>
                <div class="card card-dashboard ml-5 text-center">
                  <div class="card-body">
                    <br />

                    <h5>
                      <i class="icon-shopping-cart" style="font-size: 50px"></i
                      ><br /><br />Your wishlist is empty
                    </h5>
                  </div>
                  <!-- End .card-body -->
                </div>
                <!-- End .card-dashboard -->
              </div>
              <!-- End .col-lg-9 -->
            </div>
            <!-- End .row -->
            <% } %>

